{
  "name": "noflash/cherryhttp",
  "description": "Fast, secure, customizable and easy to use HTTP/1.1 server framework.",
  "keywords": [
    "http",
    "server",
    "framework",
    "secure",
    "fast",
    "psr-4",
    "psr-7"
  ],
  "homepage": "http://github.com/kiler129/CherryHttp",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Grzegorz Zdanowski",
      "email": "grzegorz@noflash.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/http-message": "~1.0",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1.0",
    "mikey179/vfsStream": "~1",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "suggest": {
    "noflash/shout": "Required to see any log messages from server. Also needed by most examples.",
    "ext-dio": "Required to run \"ContinuousStreamDio\" example."
  },
  "autoload": {
    "psr-4": {
      "noFlash\\CherryHttp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "noFlash\\CherryHttp\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
